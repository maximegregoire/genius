import pandas

data_df = pandas.read_csv('test.csv')
#cols_to_retain = [ 'categorical' ]
#cat_df = data_df[ cols_to_retain ]
#cat_dict = cat_df.T.to_dict().values()
onehot = pandas.get_dummies(data_df['categorical'])
data_df.insert(3,'a',onehot['a'])
data_df.insert(4,'b',onehot['b'])
data_df.insert(5,'c',onehot['c'])
del data_df['categorical']
print(data_df)
print(data_df['a'][0])

